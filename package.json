{
  "name": "bubblemap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@kokarn/react-zoom-pan-pinch": "^2.1.5",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "d3-force": "^3.0.0",
    "dotenv": "^16.3.1",
    "firebase": "^9.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys": "^2.0.0",
    "react-router-dom": "^6.14.2",
    "react-zoom-pan-pinch": "^3.1.0",
    "reactfire": "^4.2.3",
    "vite": "^4.4.4"
  },
  "overrides": {
    "react-zoom-pan-pinch": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "@kokarn/react-zoom-pan-pinch": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "scripts": {
    "start": "echo 'Loading.... This will take a few minutes.' && npm run tsc && npm-run-all --parallel dev:emulators dev:react tsc:watch",
    "start:export": "npm-run-all --parallel dev:emulators:export dev:react tsc:watch",
    "dev:react": "vite",
    "dev:emulators": "firebase emulators:start --import=./emulator-data -P testing",
    "dev:emulators:export": "firebase emulators:start --import=./emulator-data --export-on-exit -P testing",
    "tsc": "npm-run-all --parallel tsc: tsc:functions",
    "tsc:": "tsc --project ./",
    "tsc:functions": "tsc --project ./functions",
    "tsc:watch": "npm-run-all --parallel tsc:watch: tsc:watch:functions",
    "tsc:watch:": "tsc --watch --project ./",
    "tsc:watch:functions": "tsc --watch --project ./functions",
    "build": "vite build",
    "test": "firebase emulators:exec --import=./emulator-data -P testing \"vitest --passWithNoTests\"",
    "test:noemulators": "vitest --passWithNoTests",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "deploy": "firebase deploy -P app"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-tools": "^12.4.4",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "run-script-os": "^1.1.6",
    "typescript": "^5.1.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.16.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
